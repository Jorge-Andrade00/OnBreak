<Window x:Class="Onbreak2._0.ListadoCliente"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Onbreak2._0"
        mc:Ignorable="d"
        Title="ListadoCliente" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dgListadoCliente" HorizontalAlignment="Left" Height="208" Margin="45,129,0,0" VerticalAlignment="Top" Width="695"/>
        <Button x:Name="btnFiltro" Content="Filtrar" HorizontalAlignment="Left" Margin="589,58,0,0" VerticalAlignment="Top" Width="75" Click="BtnFiltro_Click"/>
        <TextBox x:Name="txtMultiUso" HorizontalAlignment="Left" Height="23" Margin="96,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="cboTipo" HorizontalAlignment="Left" Margin="253,58,0,0" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="cboActividad" HorizontalAlignment="Left" Margin="418,58,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="" HorizontalAlignment="Left" Margin="678,0,0,0" VerticalAlignment="Top" Height="94" Width="114">
            <Label.Background>
                <ImageBrush ImageSource="imagenes/logo_transparent_background.png"/>
            </Label.Background>
        </Label>

    </Grid>
</Window>
